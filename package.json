{
  "name": "dump-deps",
  "version": "1.0.0",
  "description": "Dump package dependencies to display packages with multiple versions.",
  "license": "MIT",
  "author": "Seth Bertalotto",
  "keywords": [
    "dependencies",
    "versions"
  ],
  "main": "./src/deps.js",
  "bin": "./bin/deps.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/redonkulus/dump-deps"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "optimist": "^0.6.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.1.0",
    "istanbul": "^0.4.5",
    "jenkins-mocha": "^5.0.0",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0",
    "pre-commit": "^1.0.0"
  },
  "scripts": {
    "devtest": "./node_modules/mocha/bin/_mocha test/ --recursive --reporter spec",
    "lint": "eslint --ext .js .",
    "test": "jenkins-mocha test --recursive"
  },
  "precommit": [
    "lint",
    "devtest"
  ]
}
